{
  "environment": "CartPole-v1",
  "random_seed": 0,
  "number_generations": 50,
  "use_worker_processes": true,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 250,
    "sigma": 1.0,
    "checkpoint_frequency": 10,
    "hof_size": 5
  },
  "brain": {
    "type": "LSTM_NumPy",
    "normalize_input": true,
    "normalize_input_target": 5,
    "lstm_num_layers": 3,
    "use_biases": true
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1,
    "reuse_env": true,
    "behavioral_interval": 0,
    "behavioral_max_length": 0,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0,
    "behavior_from_observation": false
  }
}
