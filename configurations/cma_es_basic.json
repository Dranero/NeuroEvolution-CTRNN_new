{
  "environment": "Qbert-ram-v0",
  "random_seed": 0,
  "number_generations": 150,
  "use_worker_processes": true,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 100,
    "sigma": 1.0,
    "checkpoint_frequency": 0,
    "hof_size": 5
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 20,
    "use_bias": false,
    "delta_t": 0.05,
    "neuron_activation": "relu",
    "neuron_activation_inplace": true,
    "normalize_input": true,
    "normalize_input_target": 10,
    "optimize_state_boundaries": "legacy",
    "clipping_range_max": 0.1,
    "clipping_range_min": -0.1,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "w_mask": "logarithmic",
    "w_mask_param": 8,
    "v_mask": "logarithmic",
    "v_mask_param": 2,
    "t_mask": "logarithmic",
    "t_mask_param": 2,
    "parameter_perturbations": 0.001
  },
  "episode_runner": {
    "type": "Standard",
    "number_fitness_runs": 1,
    "reuse_env": true,
    "keep_env_seed_fixed_during_generation": false,
    "behavioral_interval": 0,
    "behavioral_max_length": 0,
    "behavior_from_observation": false,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0
  }
}
