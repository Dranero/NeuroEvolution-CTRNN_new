{
  "environment": "LunarLander-v2",
  "random_seed": 0,
  "number_generations": 150,
  "optimizer": {
    "type": "MU_ES",
    "hof_size": 10,
    "checkpoint_frequency": 10,
    "initial_gene_range": 2,
    "tournsize": 3,
    "mu": 30,
    "extra_from_hof": 0,
    "lambda_": 600,
    "mutpb": 0.8,
    "efficiency_weight": 0.0,
    "fix_seed_for_generation": true,
    "strategy_parameter_per_gene": false
  },
  "brain": {
    "type": "ELMAN",
    "hidden_structure": [8],
    "each_state_one_hidden": true,
    "use_bias": true,
    "normalize_input": false,
    "normalize_input_target": 3,
    "optimize_initial_hidden_values": false
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 1,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0,
    "reuse_env": true,
    "use_autoencoder": false
  },
  "novelty": {
    "behavioral_interval": 10,
    "behavioral_max_length": 100,
    "behavior_source": "action",
    "distance": "euclid",
    "max_recorded_behaviors": 30,
    "recorded_behaviors_per_generation": 5,
    "novelty_nearest_k": 5,
    "novelty_weight": 0.5
  }
}
