{
  "environment": "LunarLander-v2",
  "random_seed": 0,
  "number_generations": 2,
  "use_worker_processes": true,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 10,
    "sigma": 1.0,
    "checkpoint_frequency": 0,
    "hof_size": 5
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 10,
    "neuron_activation": "relu",
    "neuron_activation_inplace": false,
    "use_bias": true,
    "delta_t": 0.5,
    "normalize_input": false,
    "normalize_input_target": 3,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "v_mask": "learned",
    "v_mask_param": 50,
    "w_mask": "learned",
    "w_mask_param": 50,
    "t_mask": "learned",
    "t_mask_param": 50,
    "parameter_perturbations": 0.01
  },
  "episode_runner": {
    "type": "Standard",
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 1,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0,
    "reuse_env": true
  },
  "novelty": {
    "behavioral_interval": 10,
    "behavioral_max_length": 100,
    "behavior_from_observation": true,
    "distance": "euclid",
    "max_recorded_behaviors": 30,
    "recorded_behaviors_per_generation": 5,
    "novelty_nearest_k": 5,
    "novelty_weight": 0.2
  }
}
