{
  "environment": "Qbert-ram-v0",
  "random_seed": 0,
  "number_generations": 3,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 3,
    "sigma": 1.0,
    "checkpoint_frequency": 10
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 5,
    "delta_t": 1.00,
    "normalize_input": false,
    "normalize_input_target": 2,
    "optimize_state_boundaries": "per_neuron",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "parameter_perturbations": 0.001,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "v_mask": "logarithmic",
    "v_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 4
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1,
    "reuse_env": true
  }
}
