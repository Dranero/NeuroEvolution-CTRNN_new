{
  "environment": "LunarLander-v2",
  "random_seed": 0,
  "number_generations": 30,
  "use_worker_processes": true,
  "optimizer": {
    "type": "MU_ES",
    "hof_size": 10,
    "checkpoint_frequency": 10,
    "initial_gene_range": 2,
    "tournsize": 7,
    "mu": 10,
    "extra_from_hof": 0,
    "lambda_": 100,
    "mutpb": 0.8,
    "efficiency_weight": 0.01
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 20,
    "neuron_activation": "tanh",
    "neuron_activation_inplace": true,
    "delta_t": 0.5,
    "use_bias": false,
    "normalize_input": true,
    "normalize_input_target": 5,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "v_mask": "logarithmic",
    "v_mask_param": 8,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 4,
    "parameter_perturbations": 0.001
  },
  "novelty": {
    "distance": "euclid",
    "max_recorded_behaviors": 30,
    "recorded_behaviors_per_generation": 5,
    "novelty_nearest_k": 5,
    "novelty_weight": 0.2,
    "behavioral_interval": 5,
    "behavioral_max_length": 500,
    "behavior_from_observation": false
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1,
    "reuse_env": true,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0
  }
}
