{
  "environment": "BipedalWalker-v3",
  "random_seed": 0,
  "number_generations": 50,
  "optimizer": {
    "type": "MU_ES",
    "population_size": 100,
    "checkpoint_frequency": 0,
    "initial_gene_range": [0.5, 1, 2, 5],
    "mate_indpb": [0.3, 0.5],
    "mutation_Gaussian_sigma_1": [1.0,0.1,0.01,0.001],
    "mutation_Gaussian_sigma_2": [1.0,0.0,0.01,0.001],
    "mutation_Gaussian_indpb_1": [1.0, 0.1, 0.01],
    "mutation_Gaussian_indpb_2": [1.0, 0.1, 0.01],
    "elitist_ratio": [0.0, 0.0, 0.05, 0.1, 0.2],
    "tournsize": [3, 5],
    "mu": [0.2, 0.3, 0.5],
    "lambda_": [1.0],
    "mutpb": [0.90, 0.8, 0.5],
    "include_parents_in_next_generation": true,
    "keep_seeds_fixed_during_generation": true,
