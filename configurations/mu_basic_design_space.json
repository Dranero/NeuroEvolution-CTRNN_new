    "v_mask": "logarithmic",
    "v_mask_param": [2, 1.5, 8],
    "t_mask": "logarithmic",
    "t_mask_param": [2, 8, 1.5],
    "parameter_perturbations": [0.0, 0.001, 0.01, 0.1]
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1,
    "reuse_env": true,
    "behavioral_interval": 0,
    "behavioral_max_length": 0
